package array;

public class Practice06 {
	public static void main(String[] args) {
		int arr[]= {26,49,16,85,63,14,97};
		int max=arr[0];
		int min=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(arr[i]>max) {
				max=min;
				min=arr[i];
			}
			else if(arr[i]<min) {
				min=arr[i];
			}
		}
		System.out.println("the max value is: "+max);
		System.out.println("the min value is: "+min);
	}
}
