package array;

import java.util.Arrays;
import java.util.Scanner;

public class Practice09 {
	public static void main(String[] args) {
		int[]value= {12,45,14,5,9,6,47,52,43,14,52};
		
		int ip=2;
		int newvalue=64;
		System.out.println("original arrays: "+Arrays.toString(value));
		for(int i=value.length;i>ip;i--) {
			value[i]=value[i-1];
		}
		value[ip]=newvalue;
		System.out.println("the new array: "+Arrays.toString(value));
	}
}
