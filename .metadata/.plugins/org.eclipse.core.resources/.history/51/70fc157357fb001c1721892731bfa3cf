package model;

import java.util.List;

import javax.persistence.*;

@Entity
@Table(name = "student")
public class Student {
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "sid")
	int sid;
	@Column(name = "sname")
	String sname;
	@ManyToMany
	@JoinTable(
			name = "student_course",
			joinColumns = @JoinColumn(name="sid"),
			inverseJoinColumns = @JoinColumn(name="cid")
			)
	List<Course> courses;
}
